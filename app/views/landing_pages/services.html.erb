<%= turbo_frame_tag "editor-replacable" do %> 
  <h1> services </h1> 
  <%= turbo_frame_tag "edit-services-section" do %>
      <section class="lp-editor__card">
        <h3 class="lp-editor__card-title">Services</h3>
        <div id="services-list">
          <%= render @landing_page.services %>
        </div>
        <div class="lp-editor__card-actions">
          <%= link_to "Add New Service",
                      new_landing_page_service_path(@landing_page),
                      class: "lp-btn lp-btn--success",
                      data: { turbo_frame: "edit-services-section" } %>
        </div>
      </section>
    <% end %>

<% end %> 
